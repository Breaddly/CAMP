      <value>Error creating work orders.</value>
    </message>
    <message id="ErrorFromSR">
      <value>Error creating work order from service request.</value>
    </message>
    <message id="ErrorFromIN">
      <value>Error creating work order from inspection.</value>
    </message>
    <message id="ErrorFromPermit">
      <value>Error creating work order from permit.</value>
    </message>
    <message id="ErrorChildWO">
      <value>Error creating child work order.</value>
    </message>
    <message id="btnCreateWO">
      <value>Create</value>
    </message>
    <message id="btnCreateWOToolTip">
      <value>Create new Work Order(s)</value>
    </message>
    <message id="btnOK">
      <value>Create</value>
    </message>
    <message id="btnOKToolTip">
      <value>Create new Work Order(s)</value>
    </message>
    <message id="btnGetFromMap">
      <value>Reload Entities</value>
    </message>
    <message id="btnGetFromMapToolTip">
      <value>Reload selected entities from the map</value>
    </message>
    <message id="btnClear">
      <value>Clear</value>
    </message>
    <message id="btnClearToolTip">
      <value>Clear selected entities</value>
    </message>
    <message id="OneForAll">
      <value>ALL selected entities</value>
    </message>
    <message id="OneForEach">
      <value>EACH selected entity</value>
    </message>
    <message id="CompleteRequiredFields" inClientLibrary="true">
      <value>Please complete required fields</value>
    </message>
    <message id="WorkActivityIdColumn">
      <value>Id</value>
    </message>
    <message id="Processing" inClientLibrary="true">
      <value>Processing...</value>
    </message>

  </messages>
</webLayoutBase>